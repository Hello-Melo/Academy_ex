package com.jafa.dto;


public class Criteria {

	//페이지
	private int page;
	private int perPageNum;

	//검색
	 String type;
	 String keyword;
	

	
	public int getPageStart() {
		return(this.page-1)*perPageNum;
	}
	
	public Criteria() {
		this.page = 1;
		this.perPageNum =10;
	}

	public void setPage(int page) {
		
		if(page <= 0) {
			this.page =1;
		}else {
			this.page=page;
		}
	}
	
}
