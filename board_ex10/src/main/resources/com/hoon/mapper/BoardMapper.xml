<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hoon.mapper.BoardMapper">
	<select id="getList" resultType="com.hoon.model.Board">	
		select * from board_tbl;
	</select>

		<insert id="insert">
			insert into board_tbl(title, contents, writer) value(#{title}, #{contents}, #{writer});
			<selectKey keyProperty="bno" order="AFTER" resultType="long">
				select last_insert_id()
			</selectKey>
		</insert>

</mapper>
